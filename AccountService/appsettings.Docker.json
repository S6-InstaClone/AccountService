{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5005"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD}"
  },
  "RabbitMQ": {
    "Host": "${RABBITMQ_HOST}",
    "Username": "${RABBITMQ_USER}",
    "Password": "${RABBITMQ_PASSWORD}"
  },
  "Keycloak": {
    "AdminUrl": "${KEYCLOAK_ADMIN_URL}",
    "Realm": "${KEYCLOAK_REALM}",
    "AdminClientId": "${KEYCLOAK_ADMIN_CLIENT_ID}",
    "AdminUsername": "${KEYCLOAK_ADMIN_USERNAME}",
    "AdminPassword": "${KEYCLOAK_ADMIN_PASSWORD}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MassTransit": "Information"
    }
  },
  "AllowedHosts": "*"
}